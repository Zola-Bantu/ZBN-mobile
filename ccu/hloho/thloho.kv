			id: ktb
			orientation: "vertical"
			size_hint_y: None
			height: 990
			BoxLayout:
				size_hint_y: None
				height: 330
				BoxLayout:
					
					orientation: "vertical"
					AsyncImage:
						id: kgw
						source: '../3f.png'
						size: self.texture_size
				
				BoxLayout:
					orientation: "vertical"
					AsyncImage:
						source: '../dend22.png'
						size: self.texture_size
				
	
			BoxLayout:
				orientation: "vertical"
				size_hint_y: None
				height: 330
				AsyncImage:
					source: '../lefatse0.png'
					size: self.texture_size
	
			BoxLayout:
				id: dikahare
				orientation: "vertical"
				size_hint_y: None
