				height: 300
				padding: (20, 10)
		
				Setswanto:
					source: '../didiriso/rn_kmt/D12.png'
					size: self.texture_size
		
			BoxLayout:
				size_hint_y: None
				height: 30
				canvas.before:
					Color:
						rgba: (.06,.45,.45, 1)
					Rectangle:
